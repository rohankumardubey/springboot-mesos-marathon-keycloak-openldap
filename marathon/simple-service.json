{
  "id": "simple-service",
  "cmd": null,
  "args": [],
  "networks": [
    {
      "mode": "container",
      "name": "springboot-mesos-marathon-keycloak-openldap_default"
    }
  ],
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "ivanfranchin/simple-service:1.0.0"
    },
    "portMappings": [
      {
        "containerPort": 8080,
        "hostPort": 0
      }
    ]
  },
  "cpus": 0.2,
  "mem": 128,
  "instances": 1,
  "env": {
    "keycloak.auth-server-url": "http://127.0.0.1:8080"
  },
  "healthChecks": [
    {
      "protocol": "HTTP",
      "port": 8080,
      "path": "/actuator/health",
      "maxConsecutiveFailures": 5,
      "gracePeriodSeconds": 300,
      "intervalSeconds": 60
    }
  ]
}